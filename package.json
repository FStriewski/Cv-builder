{
  "name": "cv-builder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/draft-js": "^0.10.32",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "draft-js": "^0.10.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha",
    "react-draggable": "^3.3.0",
    "react-icons": "^3.7.0",
    "react-outside-click-handler": "^1.2.3",
    "react-scripts-ts": "^4.0.8",
    "ts-jest": "^23.1.4"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "start2": "ts-node src/server.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "compile": "tsc",
    "build": "react-scripts-ts build",
    "test2": "react-scripts-ts test --env=jsdom",
    "test": "node_modules/.bin/jest",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "styled-components": "3.4.10",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
    "jest": {
      "bail": true,
      "verbose": false,
      "roots": [
        "__tests__",
        "src"
      ],
      "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/fileMock.js"
      },
      "transform": {
        "^.+\\.tsx?$": "ts-jest",
        "\\.(gql|graphql)$": "jest-transform-graphql"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
      "collectCoverageFrom": [
        "**/*.{ts,tsx}"
      ],
      "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/src/styles/(.*).ts",
        "/src/constants/styles.ts"
      ],
      "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
      ],
      "setupFiles": [
        "<rootDir>/jest.setup.ts"
      ]
    },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
